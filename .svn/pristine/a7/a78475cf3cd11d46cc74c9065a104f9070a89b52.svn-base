package com.damenghai.chahuitong.bean.response;

import com.damenghai.chahuitong.bean.Article;

import java.io.Serializable;
import java.util.ArrayList;

/**
 * Created by Sgun on 15/8/17.
 */
public class ArticleResponse implements Serializable {
    private ArrayList<Article> content;
    private int code;

    public ArrayList<Article> getmArticles() {
        return content;
    }

    public void setmArticles(ArrayList<Article> mArticles) {
        this.content = mArticles;
    }

    public int getCode() {
        return code;
    }

    public void setCode(int code) {
        this.code = code;
    }
}
